{
  "grammars": [
    {
      "name": "ucode",
      "camelcase": "Ucode",
      "scope": "source.uc",
      "path": ".",
      "file-types": ["uc", "ucode"],
      "highlights": [
        "queries/highlights.scm",
        "queries/highlights-jsx.scm",
        "queries/highlights-params.scm"
      ],
      "tags": ["queries/tags.scm"],
      "injection-regex": "^(uc|ucode)$"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "ucode grammar for tree-sitter",
    "authors": [
      {
        "name": "Max Brunsfeld",
        "email": "maxbrunsfeld@gmail.com"
      },
      {
        "name": "Amaan Qureshi",
        "email": "amaanq12@gmail.com"
      },
      {
        "name": "Paul White",
        "email": "phwhite@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/phwhite/tree-sitter-ucode"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
